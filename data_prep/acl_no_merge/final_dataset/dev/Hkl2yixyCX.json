    {
      "review_id": "Hkl2yixyCX",
      "rebuttal_id": "B1lldQ0bRX",
      "sentence_index": 10,
      "text": "- On Omnidirectionality:",
      "suffix": "\n",
      "coarse": "nonarg",
      "fine": "rebuttal_structuring",
      "alignment": [
