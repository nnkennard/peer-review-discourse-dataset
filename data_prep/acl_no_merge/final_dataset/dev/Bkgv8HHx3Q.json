      "coarse": "concur",
      "fine": "rebuttal_answer",
      "alignment": [
        "context_sentences",
        [
          14
        ]
      ],
      "details": {}
    },
    {
      "review_id": "Bkgv8HHx3Q",
      "rebuttal_id": "HJeB6kCJpQ",
      "sentence_index": 33,
      "text": "We only focus on some key papers on the topic.",
      "suffix": "\n\n",
      "coarse": "concur",
      "fine": "rebuttal_answer",
      "alignment": [
        "context_sentences",
        [
          14
        ]
      ],
