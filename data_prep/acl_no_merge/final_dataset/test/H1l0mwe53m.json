      "coarse": "concur",
      "fine": "rebuttal_answer",
      "alignment": [
        "context_sentences",
        [
          7
        ]
      ],
      "details": {}
    },
    {
      "review_id": "H1l0mwe53m",
      "rebuttal_id": "SyxYOL1lRm",
      "sentence_index": 12,
      "text": "Another two factors could represent long-rang and short-range correlations.",
