      "asp": "none",
      "pol": "none"
    },
    {
      "review_id": "BkeXRoYqhm",
      "sentence_index": 6,
      "text": "1.\tLack of technical novelty.",
      "suffix": "",
      "coarse": "arg_evaluative",
      "fine": "none",
      "asp": "asp_originality",
      "pol": "pol_negative"
    },
    {
      "review_id": "BkeXRoYqhm",
